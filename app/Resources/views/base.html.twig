<body>
<!-- container section start -->
<section id="container" class="">
    <header class="header dark-bg">
        <div class="toggle-nav">
            <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
        </div>
        <!--logo start-->
        <a href="{{ path('homepage') }}" class="logo">Event<span class="lite">Logger</span></a>
        <!--logo end-->
        <div class="nav search-row" id="top_menu">
            <!--  search form start -->
            <ul class="nav top-menu">
                <li>
                    <form class="navbar-form">
                        <input class="form-control" placeholder="Search" type="text">
                    </form>
                </li>
            </ul>
            <!--  search form end -->
        </div>
        <div class="top-nav notification-row">
            <!-- notificatoin dropdown start-->
            {% if app.user  %}
            <ul class="nav pull-right top-menu">
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="{{ asset('img/avatar1_small.jpg') }}">
                            </span>
                        <span class="username">{{ app.user.userprofile.family }}</span>
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu extended logout">
                        <div class="log-arrow-up"></div>
                        <li class="eborder-top">
                            <a href="{{ path('user_profile_show') }}"><i class="icon_profile"></i>Профиль</a>
                        </li>
                        <li class="eborder-top">
                            <a href="{{ path('fos_user_profile_edit') }}"><i class="icon_key_alt"></i>E-mail</a>
                        </li>
                        <li class="eborder-top">
                            <a href="{{ path('user_profile_edit') }}"><i class="icon_key_alt"></i>Аккаунт</a>
                        </li>
                        <li>
                            <a href="{{ path('fos_user_security_logout') }}"><i class="icon_key_alt"></i>Выйти</a>
                        </li>
                        <li>
                            <a href=""><i class="icon_document"></i>Инструкция</a>
                        </li>
                    </ul>
                </li>
                <!-- user login dropdown end -->
            </ul>
            <!-- notificatoin dropdown end-->
            {% endif %}
        </div>
    </header>
    <!--header end-->

    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu">
                <li class="active">
                    <a class="" href="{{ path('homepage') }}">
                        <i class="icon_house_alt"></i>
                        <span>Рабочий стол</span>
                    </a>
                </li>
                <li class="sub-menu">
                    <a class="" href="{{ path('event_index') }}">
                        <i class="icon_genius"></i>
                        <span>Мероприятия</span>
                    </a>
                </li>
                <li class="sub-menu">
                    <a href="javascript:;" class="">
                        <i class="icon_table"></i>
                        <span>Календарь</span>
                        <span class="menu-arrow arrow_carrot-right"></span>
                    </a>
                    <ul class="sub">
                        <li><a class="" href="#">Basic Table</a></li>
                    </ul>
                </li>
                {% if is_granted ('ROLE_ADMIN') %}
                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="icon_key"></i>
                            <span>Справочники</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="{{ path('region_index') }}">Области</a></li>
                            <li><a class="" href="{{ path('city_index') }}">Города</a></li>
                            <li><a class="" href="{{ path('admin_smitip_index') }}">Типы СМИ</a></li>
                            <li><a class="" href="{{ path('admin_evtip_index') }}">Типы мероприятий</a></li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="icon_key"></i>
                            <span>Таблицы</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="{{ path('admin_smi_index') }}">СМИ</a></li>
                            <li><a class="" href="#">Пользователи</a></li>
                        </ul>
                    </li>
                {% endif %}
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!--overview start-->
            <div class="row">
                <div class="col-lg-12">
                    <h3 class="page-header"><i class="fa fa-laptop"></i> Dashboard</h3>
                    <ol class="breadcrumb">
                        <li><i class="fa fa-home"></i><a class="" href="{{ path('homepage') }}">Главная</a></li>
                        <li><i class="fa fa-laptop"></i>Рабочий стол</li>
                    </ol>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-10">
                    {% block body %}
                            {% block name %} {% endblock %}
                            {% block fos_user_content %}{% endblock %}
                    {% endblock %}
                </div>
                <div class="col-md-2">Баннеры
                    <!-- List starts -->
                    <ul class="today-datas">
                        <!-- List #1 -->
                        <li>
                            <!-- Graph -->
                            <div><span id="todayspark1" class="spark"></span></div>
                            <!-- Text -->
                            <div class="datas-text">11,500 visitors/day</div>
                        </li>
                        <li>
                            <div><span id="todayspark2" class="spark"></span></div>
                            <div class="datas-text">15,000 Pageviews</div>
                        </li>
                        <li>
                            <div><span id="todayspark3" class="spark"></span></div>
                            <div class="datas-text">30.55% Bounce Rate</div>
                        </li>
                        <li>
                            <div><span id="todayspark4" class="spark"></span></div>
                            <div class="datas-text">$16,00 Revenue/Day</div>
                        </li>
                        <li>
                            <div><span id="todayspark5" class="spark"></span></div>
                            <div class="datas-text">12,000000 visitors every Month</div>
                        </li>
                    </ul>
                    <div class="text-right">
                        <div class="credits">
                            Разработка <a href="http://web-project.by">Alliance @Web Project</a><br>
                            Template <a href="https://bootstrapmade.com/">BootstrapMade</a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <!--main content end-->
</section>
<!-- container section start -->


